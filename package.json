{
  "name": "wallet-infra-sdk",
  "version": "0.0.0",
  "description": "an sdk for wallet-infra-api",
  "main": "index.js",
  "repository": "https://github.com/Brillionfi/wallet-infra-sdk.git",
  "author": "k-kaddal <khaledkaddal@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch",
    "start:dev": "ts-node src/index.ts",
    "start:prod": "node dist/index.js",
    "lint": "eslint src/.",
    "lint:fix": "eslint src/. --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,css,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "yarn lint && yarn format && yarn test && yarn build",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.4.0",
    "eslint": "^9.4.0",
    "globals": "^15.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript-eslint": "^7.11.0"
  }
}
