    "globals": "^15.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^7.11.0"
